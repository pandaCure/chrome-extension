{
  "name": "demo1",
  "manifest_version": 3,
  "version": "1.0.0.0",
  "options_page": "options.html",
  "_options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "header",
        "enabled": true,
        "path": "header.json"
      },
      {
        "id": "block",
        "enabled": true,
        "path": "block.json"
      },
      {
        "id": "mock",
        "enabled": true,
        "path": "mock.json"
      }
    ]
  },
  "host_permissions": ["<all_urls>"],
  "chrome_url_overrides": {
    "newtab": "./theme.html"
  },
  "action": {
    "default_popup": "./popup.html"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "content_scripts": [
    {
      "js": ["./content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": [
    "storage",
    "scripting",
    "topSites",
    "downloads",
    "tabs",
    "windows",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback",
    "nativeMessaging",
    "tabs"
  ]
}
